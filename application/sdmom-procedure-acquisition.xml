    <!-- Converting 'acquisitionMethod' field to Dynamic -->
    <field id="acquisitionMethod" ui-search="repeatable" mini="summary,search,list,relate" autocomplete="vocab-acquisitionMethod" ui-type="enum"/>
  </section>

</record>

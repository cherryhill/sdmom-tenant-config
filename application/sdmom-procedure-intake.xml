<record id="intake" is-extension="true" type="record,procedure" generate-services-schema="true">

  <services-record-path id="sdmom">intakes_sdmom:http://collectionspace.org/services/intake/local/sdmom,intakes_sdmom</services-record-path>

  <section id="objectEntryInformation">
    <field id="ownersRequirements" section="sdmom"></field>
    <field id="objectStatus">
        <options>
            <option id="" default="yes">Please select a value</option>
            <option id="located">Located</option>
            <option id="missing">Missing</option>
            <option id="stolen">Stolen</option>
            <option id="onloan">On Loan</option>
            <option id="returned">Returned</option>
            <option id="restricted">Restricted</option>
        </options>
    </field>
  </section>

</record>
